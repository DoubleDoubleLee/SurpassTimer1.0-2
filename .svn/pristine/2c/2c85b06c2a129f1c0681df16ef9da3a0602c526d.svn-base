#import "CCZTableHeaderView.h"
#import "CCZPokerView.h"

@implementation CCZTableHeaderView

- (instancetype)initWithFrame:(CGRect)frame
{
    self = [super initWithFrame:frame];
    if (self) {
        
        [self addView];
    }
    return self;
}

- (void)addView
{
    // 添加 Poker视图
    CCZPokerView * view = [[CCZPokerView alloc] initWithFrame:self.bounds];
    [self addSubview:view];
    
}


@end
